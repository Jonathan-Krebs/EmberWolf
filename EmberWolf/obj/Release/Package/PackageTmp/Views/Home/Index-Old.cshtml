@section Scripts{
    <script src="~/Scripts/Plugins/Range/js/ion.rangeSlider.js"></script>
    @*<script src="~/Scripts/Plugins/Masonry/masonry-docs.min.js"></script>*@
    <script src="~/Scripts/Plugins/Scrollify/Scrollify.js"></script>
    <script>

        $(function () {

            $(".js-range-slider").ionRangeSlider({
                skin: "round",
                type: "double",
                min: 0,
                max: 20,
                from: 2.5,
                to: 7.5,
                grid_snap: true,
                step: 0.5,
                prefix: "€"
            });

            $(".js-range-slider").on("change", function () {
                var $inp = $(this);
                var Value = parseFloat($inp.data("to")); // reading input value
                console.log(Value)
                var CO2Offset = Value / 10.0
                $('#OffsetValue').html(CO2Offset)



            });

            setTimeout(function () {
                $('#ScrollWheel').fadeIn(200);

                setTimeout(function () {
                    $('#ScrollWheel').fadeOut(200);
                }, 5000)
            }, 1000)
        })

        function ScrollToSection(elm) {
            Scrolling = true;
            $('section.active').removeClass('active').addClass('ShiftOut').removeClass('ShiftIn');;
            $('html, body').animate({
                scrollTop: $(elm).offset().top
            }, 800);

            setTimeout(function () {
                $('.section-indicator.active').removeClass('active')
                $('a.section-indicator[href="#' + $(elm).attr('id') + '"]').addClass('active');
                $(elm).addClass('active').addClass('ShiftIn');
                $('.ShiftOut').removeClass('ShiftOut');
                Scrolling = false;
            }, 1000)
        }

